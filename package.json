{
  "name": "bitabase-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "browserify -g uglifyify -t envify -t brfs ./src/index.js --noparse clone > public/index.bundle.js",
    "watch": "watchify -t envify -t brfs -d -o public/index.bundle.js src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "static ./public -a 0.0.0.0 --spa index.html",
    "start": "npm-run-all --parallel watch serve"
  },
  "standard": {
    "ignore": [
      "examples"
    ]
  },
  "author": "Mark Wylde",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.19.2",
    "date-fp": "^5.0.3",
    "fastn": "^2.14.1",
    "laidout": "^1.1.0",
    "on-url-change": "^1.0.2",
    "spath": "^1.3.0"
  },
  "devDependencies": {
    "envify": "^4.1.0",
    "brfs": "^2.0.2",
    "browserify": "^16.5.0",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.5",
    "predator": "^1.0.6",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  }
}
